{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "12.13.1"
  },
  "scripts": {
    "dev": "nodemon --config nodemon.json",
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:vercel": "npm run prisma:generate && npx prisma migrate deploy && next build",
    "start": "NODE_ENV=production node dist/server/index.js",
    "lint": "eslint --ext \".js,.ts,jsx,tsx\" .",
    "lint:fix": "eslint --fix --ext \".js,.ts,jsx,tsx\" .",
    "prettier:check": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "prettier:write": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "test": "jest --collect-coverage",
    "test:watch": "jest --collect-coverage --watch",
    "prepare": "cd .. && husky install",
    "lint-staged": "lint-staged",
    "prisma:generate": "npx prisma generate --schema=./prisma/schema.prisma",
    "migrate:prisma": "prisma migrate dev --schema=./prisma/schema.prisma --preview-feature --name init",
    "heroku-postbuild": "npm run build:next && npm run build:server && npm run prisma:generate"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "npm run lint:fix"
    ]
  },
  "dependencies": {
    "@prisma/client": "^2.24.1",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "fastify": "^3.20.2",
    "fastify-socket.io": "^2.0.0",
    "ioredis": "^4.27.9",
    "next": "^11.1.0",
    "next-pwa": "^5.4.0",
    "nookies": "^2.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.18.0",
    "sass": "^1.35.2",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3",
    "socket.io-redis": "^6.1.1",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.0.2",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/ioredis": "^4.27.2",
    "@types/jest": "^26.0.23",
    "@types/react": "17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redis": "^2.8.31",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-jest": "^27.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.28.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "node-filter-async": "^3.0.0",
    "node-mocks-http": "^1.10.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.1",
    "prisma": "^2.24.1",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.2.1"
  }
}
